{
  "name": "react-webpack-starter-kit",
  "version": "1.1.0",
  "description": "react-webpack-starter-kit",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- -R nyan --watch",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src/**/*.js",
    "lint:test": "eslint test/**/*.js",
    "build": "NODE_ENV=production webpack -p --config webpack/config.prod.js",
    "clean": "rm -rf public/build/",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "babel-eslint": "5.0.0",
    "eslint": "2.3.0",
    "eslint-config-qmmr": "2.0.1",
    "eslint-plugin-react": "4.2.1",
    "expect": "^1.13.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "autoprefixer": "^6.1.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "babel-register": "^6.2.0",
    "babel-runtime": "^6.2.0",
    "cors": "^2.7.1",
    "css-loader": "^0.23.0",
    "csswring": "^4.0.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "http-proxy": "^1.12.0",
    "json-loader": "^0.5.4",
    "node-libs-browser": "1.0.0",
    "nodemon": "1.9.1",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "author": "Marcin Kumorek <impressive404@gmail.com> (http://qmmr.me)",
  "license": "ISC"
}
