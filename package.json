{
  "name": "react-webpack-starter-kit",
  "version": "1.0.0",
  "description": "react-webpack-starter-kit",
  "main": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "watch": "node_modules/.bin/webpack-dev-server --hot --inline --progress --colors",
    "test": "node_modules/.bin/tape test/index.js | node_modules/.bin/faucet",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p --config webpack/config.prod.js",
    "clean": "rm -rf public/build/",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "babel-core": "5.x",
    "babel-eslint": "4.x",
    "babel-loader": "5.x",
    "cors": "2.x",
    "eslint": "^1.5.0",
    "eslint-config-qmmr": "1.x",
    "eslint-plugin-react": "3.x",
    "faucet": "^0.0.1",
    "node-libs-browser": "0.x",
    "react-hot-loader": "1.x",
    "skin-deep": "^0.6.0",
    "tape": "4.x",
    "webpack": "1.x",
    "webpack-dev-server": "1.x"
  },
  "dependencies": {
    "autoprefixer-core": "5.x",
    "babel-runtime": "5.x",
    "css-loader": "0.x",
    "csswring": "3.x",
    "ejs": "^2.3.4",
    "expose-loader": "0.x",
    "express": "4.x",
    "http-proxy": "~1.11.2",
    "json-loader": "0.x",
    "postcss-loader": "0.x",
    "react": "0.13.3",
    "style-loader": "0.x"
