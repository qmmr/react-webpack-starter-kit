{
  "name": "react-webpack-starter-kit",
  "version": "1.0.0",
  "description": "My personal boilerplate for react/webpack/redux/react-router projects",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- -R nyan --watch",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src/**/*.js",
    "lint:test": "eslint test/**/*.js",
    "build": "better-npm-run build:prod",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "clean": "rm -rf public/build/",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack --config webpack/config.dev.babel.js --progress --color",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack -p --config webpack/config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:watch": {
      "command": "",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "5.0.0",
    "better-npm-run": "0.0.8",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "dirty-chai": "1.2.2",
    "eslint": "2.4.0",
    "eslint-config-qmmr": "2.0.2",
    "eslint-plugin-react": "4.2.3",
    "expect": "1.15.2",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "sass-loader": "3.2.0"
  },
  "dependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "babel-runtime": "6.6.1",
    "cors": "2.7.1",
    "css-loader": "0.23.1",
    "csswring": "4.2.1",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "http-proxy": "1.13.2",
    "json-loader": "0.5.4",
    "node-libs-browser": "1.0.0",
    "nodemon": "1.9.1",
    "postcss-loader": "0.8.2",
    "react": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "react-dom": "0.14.7",
    "react-hot-loader": "1.3.0",
    "react-router": "2.0.1",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "author": "Marcin Kumorek <impressive404@gmail.com> (http://qmmr.me)",
  "license": "ISC"
}
